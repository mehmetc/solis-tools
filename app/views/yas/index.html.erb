<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>YASGUI</title>
  <link href="https://cdn.jsdelivr.net/npm/@triply/yasgui@4.2.28/build/yasgui.min.css" rel="stylesheet">
  <style>
      body {
          font-family: "Roboto", sans-serif;
      }

      .yasgui .autocompleteWrapper {
          display: none !important;

      }
  </style>
</head>
<body>
<div id='yasgui'></div>
<script src="//cdn.jsdelivr.net/npm/@triply/yasgui@4.2.28/build/yasgui.min.js"></script>
<script type="text/javascript">
    const yasgui = new Yasgui(document.getElementById("yasgui"),
        {
            requestConfig: {endpoint: '<%= sparql_endpoint %>'},
            copyEndpointOnNewTab: false,
        });
</script>
</body>
</html>
